<section class="content-header">
      <h1>
        Testimonial List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
        
        <li class="active">Testimonial List</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">                  
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Testimonial List</h3>
            </div>

            {{#if successMsg}}
              <div class="alert alert-success" id="result77">
                {{successMsg}}
              </div>
            {{/if}}
            <!-- /.box-header -->
            
            <div class="topbtn"><a href="/admin/testimonial/add"><button type="button" class="btn bg-purple btn-rightad">ADD</button></a></div>
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Description</th>
                  <th>Client Name</th>
                  <th>Created Date</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                    {{#each testimonial}}
                     <tr>
                      <td>{{this.description  }}</td>
                      <td>{{this.ClientName}}</td>
                      <td>{{this.createdAt}}
                      </td>
                      <td><a href="/admin/testimonial/edit/{{this.id}}" >Edit</a>&nbsp;|&nbsp;<a href="/admin/testimonial/delete/{{this.id}}" onclick="return confirm('Are you sure?')">Delete</a></td>
                    </tr>
                     {{/each}}               
                </tbody>
                
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>