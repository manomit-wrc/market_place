<script type="text/javascript">
  function isNumberKey(evt) {
    var charCode = (evt.which) ? evt.which : evt.keyCode
    return !(charCode > 31 && (charCode < 48 || charCode > 57));
  }
  function isCharacter(evt) {
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    var startPos = evt.currentTarget.selectionStart;
    if (evt.which === 32 && startPos == 0)
        evt.preventDefault();
    var isBackspaceOrDelete = (evt.keyCode == 8 || evt.keyCode == 9 || evt.keyCode == 46 || evt.keyCode == 32);
    return (isBackspaceOrDelete || (charCode > 57 && charCode <= 90));
  }
  function isBlank(evt) {
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    var startPos = evt.currentTarget.selectionStart;
    if (evt.which === 32 && startPos == 0)
        evt.preventDefault();
  }
</script>

